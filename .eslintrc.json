{
  "env": { "commonjs": true, "es6": true, "node": true, "jest/globals": true },
  "extends": ["prettier", "airbnb-base", "plugin:jest/all"],
  "parserOptions": { "ecmaVersion": 2018 },
  "rules": { "prettier/prettier": "error", "no-console": "off", "no-empty": ["error", { "allowEmptyCatch": true }]},
  "plugins": ["prettier", "jest"]
}
